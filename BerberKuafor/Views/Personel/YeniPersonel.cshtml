@model BerberKuafor.Models.Personel
@{
    ViewData["Title"] = "YeniPersonel";
    Layout = "~/Views/AnaSayfa/PersoneLayout.cshtml";
}

<form class="form-group" method="post">
    <b>@Html.LabelFor(x => x.PersonelAdi, "Personel Adı")</b>
    @Html.TextBoxFor(x => x.PersonelAdi, new  {@class="form-control"})
    <br/>
    <b>@Html.LabelFor(x => x.PersonelSoyad, "Personel Soyadı")</b>
    @Html.TextBoxFor(x => x.PersonelSoyad, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Personel Ekle</button>
</form>

